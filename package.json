{
  "name": "axios-plugin-jwt",
  "version": "1.0.0",
  "description": "Axios plugin for JWT authentication with refresh token using interceptor",
  "type": "module",
  "module": "./dist/index.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": {
        "require": "./index.d.cts",
        "default": "./index.d.ts"
      },
      "browser": {
        "require": "./dist/index.cjs",
        "default": "./dist/index.js"
      },
      "default": {
        "require": "./dist/index.cjs",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "prebuild": "bun install && rm -rf dist",
    "build": "tsup",
    "prepublish": "bun prebuild && bun build"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "tsup": "^8.4.0"
  },
  "author": "feri-irawan",
  "license": "MIT",
  "keywords": [
    "axios",
    "jwt",
    "refresh-token",
    "interceptor"
  ]
}
